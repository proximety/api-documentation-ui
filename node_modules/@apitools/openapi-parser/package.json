{
  "name": "@apitools/openapi-parser",
  "author": "Mrinmoy Majumdar",
  "license": "MIT",
  "version": "0.0.25",
  "description": "Resolve OpenAPI Refs and/or convert Swagger 2.0 to OpenAPI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrin9/apitools.git",
    "directory": "packages/openapi-parser"
  },
  "module": "dist/openapi-parser.es.js",
  "main": "dist/openapi-parser.umd.js",
  "exports": {
    ".": {
      "import": "./dist/openapi-parser.es.js",
      "require": "./dist/openapi-parser.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest",
    "test-debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "swagger-client": "^3.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-eslint": "^8.0.2",
    "babel-jest": "^28.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.5",
    "jest": "^28.1.0",
    "vite": "^2.9.9"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "openapi",
    "parser",
    "swagger2openapi",
    "swagger-converter",
    "dereference",
    "json",
    "yaml",
    "swagger-parser"
  ]
}
